import java.awt.Color;
import javax.swing.*;
import java.util.Random;


public class ColorGuessingGame {
	
	public String globalColor;

	public static void main(String[] args) {
	
		Random ranNum = new Random();

	}
	
	public static Color chooseColor(int color)
	{
		switch(color)
		{
		case(1):
			return Color.BLUE;
			break;
		case(2):
			return Color.YELLOW;
			break;
		case(3):
			return Color.RED;
			break;
		case(4):
			return Color.GREEN;
			break;
		case(5):
			return Color.ORANGE;
			break;
		case(6):
			return Color.CYAN;
			break;
		}
	}
}
